# si el modulo existe
<ifModule mod_rewrite.c>
	# encendemos el modulo
	RewriteEngine on 
	# si el file no existe
	RewriteCond %{REQUEST_FILENAME} !-f 
	# ejecuta la url que esta aqui
	RewriteRule ^(.*)$ index.php?url=$1 [QSA,L] 
</IfModule>

